{
  "release-type": "rust",
  "packages": {
    ".": {
      "component": "root"
    },
    "cli": {
      "component": "cli"
    },
    "commons": {
      "component": "commons"
    },
    "core": {
      "component": "core"
    },
    "inline": {
      "component": "inline"
    },
    "parser": {
      "component": "parser"
    },
    "render": {
      "component": "render"
    }
  },
  "plugins": [
    {
      "type": "cargo-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "unimarkup",
      "components": [
        "root",
        "cli",
        "commons",
        "inline",
        "parser",
        "render"
      ]
    }
  ],
  "include-component-in-tag": false,
  "bump-minor-pre-major": true,
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "arch",
      "section": "Architecture/Refactor",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Miscellaneous",
      "hidden": true
    }
  ]
}